{
  "home": {
    "container": {
      "display": "flex",
      "flexDirection": "column",
      "justifyContent": "center",
      "height": "calc(100vh - 115px)",
      "paddingTop": "40px",
      "paddingLeft": "15px",
      "paddingRight": "15px",
      "paddingBottom": "15px",
      "backgroundColor": "#555555"
    },
    "search": {
      "container": {
        "display": "flex",
        "flexDirection": "column",
        "justifyContent": "center",
        "height": "40%"
      },
      "header": {
        "textAlign": "center",
        "color": "#F8F4EC"
      },
      "input": {
        "display": "block",
        "height": "45px",
        "width": "60%",
        "marginTop": "50px",
        "marginBottom": "0",
        "marginLeft": "auto",
        "marginRight": "auto",
        "paddingLeft": "5px",
        "paddingRight": "5px",
        "color": "#111111",
        "backgroundColor": "#F8F4EC",
        "fontSize": "20px",
        "border": "4px solid #333333",
        "outline": "none"
      }
    },
    "cards": {
      "rail": {
        "width": "100%",
        "height": "60%",
        "overflow": "auto"
      },
      "container": {
        "display": "flex",
        "justifyContent": "center",
        "alignItems": "center",
        "width": "calc(100% - 30px)",
        "paddingTop": "15px",
        "paddingRight": "15px",
        "paddingBottom": "15px",
        "paddingLeft": "15px",
        "flexWrap": "wrap"
      },
      "card": {
        "container": {
          "width": "200px",
          "height": "200px",
          "margin": "5px"
        },
        "header": {

        },
        "body": {
          "position": "relative",
          "height": "100%",
          "width": "100%"
        },
        "image": {
          "width": "200px",
          "height": "200px",
          "cursor": "pointer"
        },
        "footer": {

        }
      }
    }
  },
  "footer": {
    "container": {
      "display": "flex",
      "height": "60px",
      "backgroundColor": "#444444",
      "justifyContent": "center",
      "alignItems": "center",
      "color": "#F8F4EC"
    },
    "spotify": {
      "position": "absolute",
      "right": "25px",
      "width": "100px"
    }
  }
}
